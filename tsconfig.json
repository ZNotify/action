{
    "compilerOptions": {
        "target": "es2019",
        "module": "ESNext",
        "lib": [
            "es2019"
        ],
        "declaration": true,
        "declarationMap": true,
        "importHelpers": false,
        "downlevelIteration": false,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true
    }
}